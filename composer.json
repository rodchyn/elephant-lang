{
    "name": "rodchyn/elephant-lang",
    "type": "library",
    "description": "Sugar syntax for PHP language",
    "keywords": ["syntax", "language"],
    "homepage": "http://elephant-lang.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Yura Rodchyn",
            "email": "rodchyn@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/console": "@stable"
    },
    "require-dev": {
        "symfony/dependency-injection": "*"
    },
    "target-dir": "Rodchyn/ElephantLang",
    "autoload": {
        "psr-0": {"Rodchyn\\ElephantLang\\": "src/"},
        "classmap": ["lib/"],
        "files": ["autoload.php"]
    },
    "prefer-stable": true
}
